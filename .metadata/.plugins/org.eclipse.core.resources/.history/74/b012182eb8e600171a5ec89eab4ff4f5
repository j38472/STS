package com.bxd.one;

import java.awt.*;
public class AWTDemo {

	public static void main(String[] args) {
		Frame f = new Frame("my awt");
		f.setSize(500, 400);//窗口的长宽
		f.setLocation(300, 100);//窗口的弹出位置
		f.setVisible(true);//窗口可见
		Button b = new Button("按钮");
		b.setSize(50, 80);
		b.setVisible(true);
		f.add(b);
	}

}
