package com.csdn.text;
/**
 * 创建两个线程，其中一个输出1-52，另外一个输出A-Z。输出格式要求：12A 34B 56C 78D
 * @author Administrator
 *
 */
public class Demo2 {
	
	public static void main(String[] args) {
		Runnable r1 = new Runnable() {
			@Override
			public void run() {
				for (int i = 1; i < 53; i++) {
					System.out.print(i);
				}
			}
		};
		Runnable r2 = new Runnable() {
			
			@Override
			public void run() {
				for (int i = 65; i <91 ; i++) {
					System.out.print((char)i);
				}
				
			}
		};
		new Thread(r1).start();
		new Thread(r2).start();
	}
}
