
public class ThreadTest {
	public static void main(String[] args) {
		new Thread(){
			public void run(){
				for (int i = 0; i < 100; i++) {
					System.out.println(Thread.currentThread().toString()+".........."+i);
				}
			}
		}.start();
		for (int i = 0; i < 100; i++) {
			System.out.println(Thread.currentThread().toString()+".........."+i);
		}
		for (int i = 0; i < 100; i++) {
			System.out.println(Thread.currentThread().toString()+".........."+i);
		}
	}
}
