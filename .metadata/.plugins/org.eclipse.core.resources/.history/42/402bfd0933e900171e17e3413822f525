<style type="text/css">
div {
	width: 200px;
	height: 300px;
	float: left;
	border: 3px;
	margin: 20px;
}
</style>
<script type="text/javascript">
	window.onload = function() {
		var aDiv = document.getElementsByTagName('div');
		//alert(aDiv.length);
		var i = 0;
		while (i < aDiv.length) {
			aDiv[i].style.background = "red";
			i++;
		}
	};
</script>
