	public static void main(String[] args) {
		Set s = new HashSet<String>();
		s.add("aaa");
		s.add("bbb");
		s.add("aaa");
		System.out.println(s.size());
		copyIO c = new copyIO();
		try {
			c.myIO();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
/**
 * 复制图片
 * f盘的ABCD文件夹中A复制出B
 * @author Administrator
 *	思路
 *	1.图片复制需要字节流
 *	
 */
class copyIO{
	private void mian() throws IOException {
	}
	public static void myIO() throws IOException{

//		File f = new File("F:\\ABCD\\A.jpg");
		FileInputStream fin = new FileInputStream(new File("F:\\demo.txt"));
		FileOutputStream fout = new FileOutputStream(new File("F:\\demo1.txt"));
		byte [] line = new byte [1024];
		while (fin.read(line)!=-1) {
			fout.write(line, 0, line.length);
		}
		fout.close();
		fin.close();
	
	}
}