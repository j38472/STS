package com.zgcxueyuan.lsm;

import java.io.File;

public class FileDemo {

	public static void main(String[] args) {
		demo("f:\\");
		
		
	}

	public static void demo(String path){
		File dir = new  File(path);
		File [] files = dir.listFiles();
		for (File f : files) {
			System.out.println(f.getName());
			if (f.isDirectory()) {
				demo(f.getPath());
			}
		}
		
	}


}
