package chenyuanhao_b;

public class Main {

	public static void main(String[] args) {
		StudentList sl = new StudentList();
		Student stu0 = new Student(5, "张三");
		Student stu1 = new Student(10, "李四");
		Student stu2 = new Student(17, "王五");
		Student stu3 = new Student(33, "陈五");
		sl.add(stu0);
		sl.add(stu1);
		sl.add(stu2);
		System.out.println(sl.students[0].getName());
		System.out.println(sl.length());
//		sl.remove(1);
		System.out.println(sl.length());
		sl.insert(2, stu3);
		System.out.println(sl.toString());
		System.out.println(sl.getStudent(1));
	}
}
