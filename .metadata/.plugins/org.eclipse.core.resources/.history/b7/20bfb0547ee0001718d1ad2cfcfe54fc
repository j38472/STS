package com.zgcxueyuan.day3;
/**
 * 人物类
 * @author Administrator
 *
 */
public class Figure {
	int length = 0 ;
	int board [] =new int [length];
	int count =  length ;//尾部指针
	/**
	 * 
	 * @param isLandlord true 是地主    false 不是地主
	 */
	public Figure(boolean isLandlord) {
		if (isLandlord==true) {
			length = 22 ;
		}else {
			length = 18 ;
		}
	} 

	
}
